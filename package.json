{
  "name": "sfftfth",
  "version": "1.0.1",
  "description": "A simple static site generator",
  "main": "index.js",
  "scripts": {
    "test": "eslint lib/**/*.js test/**/*.js && nyc tape test/*.test.js",
    "travis-coverage": "nyc report --reporter=text-lcov tape/*.test.js | coveralls",
    "coverage": "nyc report --reporter=lcov tape/*.test.js",
    "prepublish": "babel -d build lib/*.js index.js && cp package.json build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dak0rn/sfftfth.git"
  },
  "bin": {
    "sfftfth": "build/index.js"
  },
  "keywords": [
    "static",
    "site",
    "generator"
  ],
  "author": "Daniel Koch <daniel@suitsoft.eu>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/dak0rn/sfftfth/issues"
  },
  "homepage": "https://github.com/dak0rn/sfftfth#readme",
  "dependencies": {
    "argparse": "^1.0.7",
    "front-matter": "^2.0.8",
    "fs-promise": "^0.5.0",
    "glob-promise": "^1.0.6",
    "immutable": "^3.8.1",
    "marked": "^0.3.5",
    "mkdirp": "^0.5.1",
    "moment": "^2.13.0",
    "pygmentize-bundled": "^2.3.0",
    "recursive-copy": "^2.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.11.0",
    "nyc": "^6.4.4",
    "prunk": "^1.2.1",
    "tape": "^4.5.1"
  }
}
